{
  "name": "spearwolf-js",
  "version": "0.0.7",
  "description": "various javascript helpers and classes for various three.js/webgl experiments!",
  "main": "dist/spearwolf.js",
  "module": "build/index.js",
  "types": "src/index.ts",
  "sideEffects": false,
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build:umd": "cross-env NODE_ENV=production rollup -c scripts/rollup.config.umd.js",
    "build:esm-bundle": "cross-env NODE_ENV=production rollup -c scripts/rollup.config.esm.js",
    "build:esm": "cross-env BABEL_ENV=production babel src/ --extensions \".ts\" --out-dir ./build/ --source-maps",
    "watch:esm": "cross-env BABEL_ENV=production babel src/ --extensions \".ts\" --out-dir ./build/ --source-maps --watch",
    "build": "run-p build:*",
    "prepublishOnly": "run-p build:*",
    "clean": "rimraf build coverage dist",
    "start": "webpack-dev-server",
    "test:eslint": "eslint .",
    "test:karma": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "test:types": "tsc -p ./tests/types/tsconfig.json",
    "test": "npm-run-all --parallel test:eslint test:types --serial test:karma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spearwolf/spearwolf-js.git"
  },
  "author": "Wolfger Schramm <wolfger@spearwolf.de>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/spearwolf/spearwolf-js/issues"
  },
  "homepage": "https://github.com/spearwolf/spearwolf-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "assert": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.1.4",
    "core-js": "^3.1.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-problems": "^3.0.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.13.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.9.0",
    "rollup-plugin-terser": "^5.0.0",
    "three": "^0.105.1",
    "typescript": "^3.5.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  },
  "peerDependencies": {
    "three": "^0.104.0"
  }
}
