{
  "name": "spearwolf-js",
  "version": "0.0.1",
  "description": "various javascript helpers and classes for various three.js/webgl experiments!",
  "main": "dist/spearwolf.js",
  "module": "dist/esm/index.js",
  "types": "types/index.d.ts",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build:umd": "cross-env NODE_ENV=production rollup -c scripts/rollup.config.umd.js",
    "build:esm-bundle": "cross-env NODE_ENV=production rollup -c scripts/rollup.config.esm.js",
    "build:esm": "cross-env NODE_ENV=production babel src --out-dir dist/esm --source-maps --ignore \"src/**/*.spec.js\",\"src/**/*.test.js\"",
    "build": "run-p build:*",
    "prepublishOnly": "run-p build:*",
    "clean": "rimraf build coverage dist",
    "start": "webpack-dev-server",
    "test:eslint": "eslint .",
    "test:karma": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "test:types": "tsc -p ./types/test/tsconfig.json",
    "test": "run-p build:*",
    "watch:esm": "npm run build:esm -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spearwolf/spearwolf-js.git"
  },
  "author": "Wolfger Schramm <wolfger@spearwolf.de>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/spearwolf/spearwolf-js/issues"
  },
  "homepage": "https://github.com/spearwolf/spearwolf-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "assert": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "eslint-config-problems": "^2.0.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "rollup-plugin-terser": "^4.0.2",
    "three": "^0.100.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "peerDependencies": {
    "three": "^0.100.0"
  }
}
